MODULE SYSTEM;

TYPE WCHAR*=SYSTEM_MOD_TYPE_PRIMITIVE_WCHAR;

TYPE WSTRING*=RECORD END;

TYPE COMMAND_LINE_PARAMETER*=WSTRING;

TYPE COMMAND_LINE*=RECORD parameter-: COMMAND_LINE_PARAMETER; next-: POINTER TO COMMAND_LINE END;

TYPE COMMAND_LINE_PTR*=POINTER TO COMMAND_LINE; 

PROCEDURE GetCommandLine*(): COMMAND_LINE_PTR = PROCEDURE_PRIMITIVE_GetCommandLine; END GetCommandLine;

END SYSTEM.